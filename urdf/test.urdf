        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707095 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </collision>
    </link>

    <joint name="left_front_joint" type="continuous">
        <origin xyz="0.4 0.3 0.1" rpy="0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="left_front"/>
        <axis xyz="0.0 1 0.0"/>
    </joint>

 

  <gazebo reference="left_front">
    <material>Gazebo/Black</material>
  </gazebo>

 <link name="left_back">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.5"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="1.5707095 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
            <material name="black">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707095 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </collision>
    </link>

    <joint name="left_back_joint" type="continuous">
        <origin xyz="-0.4 0.3 0.1" rpy="0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="left_back"/>
        <axis xyz="0.0 1 0.0"/>
    </joint>

 

  <gazebo reference="left_back">
    <material>Gazebo/Black</material>
  </gazebo>

 <link name="right_back">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.5"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="1.5707095 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
            <material name="black">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707095 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </collision>
    </link>

    <joint name="right_back_joint" type="continuous">
        <origin xyz="-0.4 -0.3 0.1" rpy="0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="right_back"/>
        <axis xyz="0.0 1 0.0"/>
    </joint>

 

  <gazebo reference="right_back">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="base_link">
    <material>Gazebo/Turquoise</material>
    <pose>0 0 3 0 0 0</pose>
  </gazebo>

  <joint name="dummy_joint" type="fixed">
    <parent link="dummy"/>
    <child link="base_link"/>
  </joint>
</robot>